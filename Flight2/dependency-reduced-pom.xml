<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.cs6240.csj</groupId>
  <artifactId>hw4</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.3.0</version>
        <executions>
          <execution>
            <id>Secondary</id>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer>
                  <mainClass>hw4.Secondary</mainClass>
                </transformer>
              </transformers>
              <finalName>SECONDARY</finalName>
            </configuration>
          </execution>
          <execution>
            <id>HPopulate</id>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer>
                  <mainClass>hw4.HPopulate</mainClass>
                </transformer>
              </transformers>
              <finalName>HPOPULATE</finalName>
            </configuration>
          </execution>
          <execution>
            <id>HPopulateV1</id>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer>
                  <mainClass>hw4.HPopulateV1</mainClass>
                </transformer>
              </transformers>
              <finalName>HPOPULATEV1</finalName>
            </configuration>
          </execution>
          <execution>
            <id>HPopulateV2</id>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer>
                  <mainClass>hw4.HPopulateV2</mainClass>
                </transformer>
              </transformers>
              <finalName>HPOPULATEV2</finalName>
            </configuration>
          </execution>
          <execution>
            <id>HCompute</id>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer>
                  <mainClass>hw4.HCompute</mainClass>
                </transformer>
              </transformers>
              <finalName>HCOMPUTE</finalName>
            </configuration>
          </execution>
          <execution>
            <id>HComputeV2</id>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer>
                  <mainClass>hw4.HComputeV2</mainClass>
                </transformer>
              </transformers>
              <finalName>HCOMPUTEV2</finalName>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <properties>
    <maven.compiler.target>1.8</maven.compiler.target>
    <maven.compiler.source>1.8</maven.compiler.source>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <hadoop.version>3.3.3</hadoop.version>
    <hbase.version>2.4.15</hbase.version>
  </properties>
</project>
